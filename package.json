  },
  "bugs": {
    "url": "https://github.com/toviszsolt/react-clsx/issues"
  },
  "license": "MIT",
  "scripts": {
    "build": "yarn test && node scripts/build.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "clsx": "^2.1.1",
    "esbuild": "^0.27.2",
    "jsdom": "^27.4.0",
    "react": "^19.2.3",
    "vitest": "^4.0.16"
  }
